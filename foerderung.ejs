```{=html}

<div class="grid text-center" style="--bs-gap: 1rem;">
    <% for (const item of items) { %>
      <div class="g-col-12 g-col-md-6">
        <a href="<%= item.website %>">
          <img class="img-fluid" src="<%= item.img %>" alt="<%= item.title %>"/>
        </a>
      </div>
    <% } %>
</div>

